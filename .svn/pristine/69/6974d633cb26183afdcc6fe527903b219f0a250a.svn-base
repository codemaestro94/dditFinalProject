	<select id="frcsMenuList" resultType="FrcsMenuVO">
		SELECT
		    a.menu_cd
		  , a.frcs_id
		  , a.sale_yn
		  , b.MENU_NAME
		  , b.MENU_PRICE
		  , b.MENU_DES
		  , b.MENU_CG
		  , b.MENU_STATE
		  , b.MENU_RSDATE
		  , b.MENU_DELDATE
		  , b.TABLE_NAME
		FROM
		    frcs_menu a JOIN head_menu b 
		ON a.menu_cd = b.menu_cd
		where a.frcs_id = 'aaabbb2'
	</select>
	
	<select id="resfrcsMenuList" parameterType="string" resultType="FrcsMenuVO">
		SELECT
		    a.menu_cd
		  , a.frcs_id
		  , a.sale_yn
		  , b.MENU_NAME
		  , b.MENU_PRICE
		  , b.MENU_DES
		  , b.MENU_CG
		  , b.MENU_STATE
		  , b.MENU_RSDATE
		  , b.MENU_DELDATE
		  , b.TABLE_NAME
		FROM
		    frcs_menu a left join head_menu b 
			on a.menu_cd = b.menu_cd
			and a.frcs_id = #{frcsId}
	</select>

